doctype html
html
	head
		title create page
		meta(charset='utf-8')
	body
		header
			h1
				a(href='/dbase') CITY. Where is.
			ul
				each list in _dbase
					li
						a(href='/dbase/'+list.title)= list.title
		article
			form(action='/dbase/create' method='post')
				p
					input(type='text' name='title_' placeholder='title')
				p
					textarea(name='descr_' placeholder='description')
				p
					input(type='submit' value='create')
