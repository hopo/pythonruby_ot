doctype html
html
	head
		title delete page
		meta(charset='utf-8')
	body
		header
			h1
				a(href='/dbase') CITY. Where is.
			ul
				each list in _dbase
					li
						a(href='/dbase/'+list.title)= list.title
		article
			- id=encodeURIComponent(_dbase2['@rid'])
			form(action='/dbase/'+id+'/delete' method='post')
				p= '['+_dbase2.title+'] Do you want to delete???'
				p
					input(type='submit' value='YES')
					a(href='/dbase/'+_dbase2.title)  cancel 
